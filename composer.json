{
    "name": "rpeterson/laravel-fdb",
    "description": "FoundationDB support for Laravel",
    "repositories": [
      {
        "type": "git",
        "url": "https://github.com/webpatser/laravel-uuid"
      }
    ],
    "require": {
        "laravel/framework": "~4.2",
        "webpatser/laravel-uuid": "1.*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Ryan J. Peterson",
            "email": "rpeterson@serenitysoft.com"
        }
    ],
    "autoload": {
      "psr-4": {
        "FDB\\": "src/FDB/"
      }
    },
    "config": {
      "preferred-install": "dist"
    }
}
